#include "animations.h"

void animation1(void){
	for(int i=0; i<3; i++){
		LED_OFF(i);
		HAL_Delay(200);
		LED_ON(i);
		HAL_Delay(200);
		LED_OFF(i);
		HAL_Delay(200);
		LED_TOGGLE(i);
		HAL_Delay(200);
		LED_OFF(i);
		HAL_Delay(200);
	}
}
